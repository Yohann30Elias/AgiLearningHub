{
  "sap.app": {
    "id": "agilh",
    "type": "application",
    "title": "AGI Learning Hub"
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "agilh.view.App",
      "type": "XML",
      "async": true,
      "id": "appShell"
    },
    "dependencies": {
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {}
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "agilh.view",
        "controlId": "appNav",
        "controlAggregation": "pages",
        "async": true
      },
      "routes": [
        { "name": "landing", "pattern": "", "target": "landing" },
        { "name": "catalog", "pattern": "catalog", "target": "catalog" },
        { "name": "assigned", "pattern": "assigned", "target": "assigned"},
        { "name": "mylearnings", "pattern": "mylearnings", "target": "mylearnings" },
        { "name": "favorites", "pattern": "favorites", "target": "favorites" },
        { "name": "completed", "pattern": "completed", "target": "completed" },
        { "name": "manager", "pattern": "manager", "target": "manager" },
        { "name": "profile", "pattern": "profile", "target": "profile" }
      ],
      "targets": {
        "landing":   { "viewName": "Landing",     "viewId": "landing",     "viewLevel": 1 },
        "catalog":   { "viewName": "Catalog",      "viewId": "catalog",      "viewLevel": 2 },
        "assigned":   { "viewName": "Assigned",      "viewId": "assigned",      "viewLevel": 2 },
        "mylearnings": { "viewName": "MyLearnings","viewId": "mylearnings",  "viewLevel": 2 },
        "favorites":   { "viewName": "Favorites",      "viewId": "favorites",      "viewLevel": 2 },
        "completed":   { "viewName": "Completed",      "viewId": "completed",      "viewLevel": 2 },
        "manager":   { "viewName": "Manager",      "viewId": "manager",      "viewLevel": 2 },
        "profile":   { "viewName": "Profile",      "viewId": "profile",      "viewLevel": 2 }
      }
    }
  }
}
